#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/misky/Documents/SIDE_PROJECTS/JS/nodejs/new-html2pdf/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/misky/Documents/SIDE_PROJECTS/JS/nodejs/new-html2pdf/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@playwright+test@1.24.1/node_modules/@playwright/test/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@playwright+test@1.24.1/node_modules/@playwright/test/cli.js" "$@"
fi
